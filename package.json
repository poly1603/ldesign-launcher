{
  "name": "@ldesign/launcher",
  "type": "module",
  "version": "2.0.0",
  "description": "零配置前端项目启动器 - 自动检测13+框架，基于Vite 5.0+，开箱即用的dev/build/preview工具",
  "keywords": [
    "vite",
    "launcher",
    "frontend",
    "build-tool",
    "dev-server",
    "typescript",
    "ldesign",
    "zero-config",
    "auto-detect",
    "react",
    "vue",
    "svelte",
    "angular",
    "solid",
    "preact",
    "multi-framework",
    "vite5"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/launcher#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/launcher.git",
    "directory": "packages/launcher"
  },
  "bugs": {
    "url": "https://github.com/ldesign/launcher/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js",
      "require": "./dist/cli/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "import": "./dist/core/*.js",
      "require": "./dist/core/*.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.cjs"
    },
    "./cli/commands": {
      "types": "./dist/cli/commands/index.d.ts",
      "import": "./dist/cli/commands/index.js",
      "require": "./dist/cli/commands/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./client/react": {
      "types": "./dist/client/react/useAppConfig.d.ts",
      "import": "./dist/client/react/useAppConfig.js"
    },
    "./client/vue": {
      "types": "./dist/client/vue/index.d.ts",
      "import": "./dist/client/vue/index.js"
    },
    "./client/vue2": {
      "types": "./dist/client/vue2/useAppConfig.d.ts",
      "import": "./dist/client/vue2/useAppConfig.js"
    },
    "./client/svelte": {
      "types": "./dist/client/svelte/useAppConfig.d.ts",
      "import": "./dist/client/svelte/useAppConfig.js"
    },
    "./client/solid": {
      "types": "./dist/client/solid/useAppConfig.d.ts",
      "import": "./dist/client/solid/useAppConfig.js"
    },
    "./client/qwik": {
      "types": "./dist/client/qwik/useAppConfig.d.ts",
      "import": "./dist/client/qwik/useAppConfig.js"
    },
    "./client/lit": {
      "types": "./dist/client/lit/useAppConfig.d.ts",
      "import": "./dist/client/lit/useAppConfig.js"
    },
    "./client/angular": {
      "types": "./dist/client/angular/useAppConfig.d.ts",
      "import": "./dist/client/angular/useAppConfig.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "launcher": "./bin/launcher.js",
    "ldesign-launcher": "./bin/launcher.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:js && npm run build:types",
    "build:js": "cross-env NODE_OPTIONS=\"--max-old-space-size=32768\" tsup --no-dts",
    "build:dts": "cross-env NODE_OPTIONS=\"--max-old-space-size=32768\" tsup --dts-only",
    "build:types": "tsc -p tsconfig.build.json",
    "build:tsc": "tsc --noEmit false",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:unit": "vitest run -c vitest.unit.config.ts",
    "test:all": "vitest run",
    "bench": "vitest bench",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit fix",
    "deps:check": "depcheck",
    "deps:update": "ncu -u",
    "prepublishOnly": "pnpm run build && pnpm run test:run",
    "size": "size-limit",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "benchmark": "tsx scripts/benchmark.ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@vitejs/plugin-vue2": "^2.3.4",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.3",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "inquirer": "^9.2.12",
    "jiti": "^2.6.1",
    "json5": "^2.2.3",
    "open": "^10.1.0",
    "ora": "^7.0.1",
    "picocolors": "^1.1.1",
    "vite": "^7.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@size-limit/preset-small-lib": "^11.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "@types/qrcode": "^1.5.6",
    "@vitest/ui": "^4.0.9",
    "cross-env": "^10.1.0",
    "eslint": "^9.18.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "rimraf": "^5.0.5",
    "size-limit": "^11.0.0",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.9"
  },
  "peerDependencies": {
    "@builder.io/qwik": "^1.11.0",
    "@marko/vite": "^1.0.0",
    "@preact/preset-vite": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue2": "^2.3.3",
    "vite-plugin-solid": "^2.10.0"
  },
  "optionalDependencies": {
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0"
  },
  "peerDependenciesMeta": {
    "@vitejs/plugin-legacy": {
      "optional": true
    },
    "@vitejs/plugin-react": {
      "optional": true
    },
    "@vitejs/plugin-vue": {
      "optional": true
    },
    "@vitejs/plugin-vue2": {
      "optional": true
    },
    "@sveltejs/vite-plugin-svelte": {
      "optional": true
    },
    "@analogjs/vite-plugin-angular": {
      "optional": true
    },
    "@angular-devkit/build-angular": {
      "optional": true
    },
    "vite-plugin-solid": {
      "optional": true
    },
    "@preact/preset-vite": {
      "optional": true
    },
    "@builder.io/qwik": {
      "optional": true
    },
    "@marko/vite": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
