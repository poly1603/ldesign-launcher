{
  "name": "@ldesign/launcher",
  "type": "module",
  "version": "1.0.0",
  "description": "基于 Vite JavaScript API 的前端项目启动器，提供统一的开发服务器、构建工具和预览服务",
  "keywords": [
    "vite",
    "launcher",
    "frontend",
    "build-tool",
    "dev-server",
    "typescript",
    "ldesign"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/launcher#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/launcher.git",
    "directory": "packages/launcher"
  },
  "bugs": {
    "url": "https://github.com/ldesign/launcher/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js",
      "require": "./dist/cli/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "import": "./dist/core/*.js",
      "require": "./dist/core/*.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.cjs"
    },
    "./cli/commands": {
      "types": "./dist/cli/commands/index.d.ts",
      "import": "./dist/cli/commands/index.js",
      "require": "./dist/cli/commands/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "launcher": "./bin/launcher.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" NODE_ENV=production tsup",
    "build:js": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" tsup --no-dts",
    "build:dts": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" tsup --dts-only",
    "build:tsc": "tsc",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:unit": "vitest run -c vitest.unit.config.ts",
    "test:all": "vitest run",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint -c .eslintrc.js \"src/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run typecheck"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.3",
    "cli-progress": "^3.12.0",
    "cac": "^6.7.14",
    "commander": "^11.1.0",
    "fast-glob": "^3.3.2",
    "figlet": "^1.7.0",
    "glob": "^10.3.10",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "jiti": "^2.0.0",
    "json5": "^2.2.3",
    "mkcert": "^3.2.0",
    "node-fetch": "^3.3.2",
    "node-notifier": "^10.0.1",
    "open": "^10.1.0",
    "ora": "^7.0.1",
    "picocolors": "^1.0.0",
    "qrcode": "^1.5.4",
    "svg2ttf": "^6.0.3",
    "svgicons2svgfont": "^12.0.0",
    "tar": "^7.4.3",
    "ttf2eot": "^3.1.0",
    "ttf2woff": "^3.0.0",
    "ttf2woff2": "^5.0.0",
    "ws": "^8.18.3",
    "yauzl": "^3.0.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.11",
    "@types/node-notifier": "^8.0.5",
    "@types/qrcode": "^1.5.5",
    "@types/tar": "^6.1.13",
    "@types/ws": "^8.5.10",
    "@types/yauzl": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitest/ui": "^1.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.55.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.3.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitepress": "^1.6.4",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue2": "^2.3.3",
    "vite": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": false
    },
    "@vitejs/plugin-legacy": {
      "optional": true
    },
    "@vitejs/plugin-react": {
      "optional": true
    },
    "@vitejs/plugin-vue": {
      "optional": true
    },
    "@vitejs/plugin-vue2": {
      "optional": true
    },
    "@sveltejs/vite-plugin-svelte": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
